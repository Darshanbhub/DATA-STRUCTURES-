#include <stdio.h>

int main()
{
    int x,y;
    printf("Enter a number\n");
    printf("Enter a number\n");
    printf("Enter another number to be sapped with\n");
    scanf("%d", &y);
    
    void swap(int *x, int *y)
    {
        int temp;
        temp = *x;
        *x = *y;
        *y = temp;
        
    }
    swap(&x, &y);
    printf("Numbers after swapping are \n %d\n %d", x, y );
    
    return 0;
}
